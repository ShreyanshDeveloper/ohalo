{
  "extends": ["next/core-web-vitals", "prettier"],
  "rules": {
    "no-unused-vars": [
      "error",
      {
        "vars": "all",
        "args": "all",
        "ignoreRestSiblings": false,
        "argsIgnorePattern": "^_"
      }
    ],
    "no-dupe-keys": "error"
  },
  "ignorePatterns": ["src/lib/types/graphql.ts"]
}
